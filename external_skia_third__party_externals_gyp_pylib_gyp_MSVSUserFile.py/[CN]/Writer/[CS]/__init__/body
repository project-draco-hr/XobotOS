def __init__(self, user_file_path, version, name):
    'Initializes the user file.\n\n    Args:\n      user_file_path: Path to the user file.\n      version: Version info.\n      name: Name of the user file.\n    '
    self.user_file_path = user_file_path
    self.version = version
    self.name = name
    self.configurations = {}
