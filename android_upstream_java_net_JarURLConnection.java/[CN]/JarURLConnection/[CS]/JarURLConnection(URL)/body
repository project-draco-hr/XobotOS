{
  super(url);
  file=url.getFile();
  int sepIdx;
  if ((sepIdx=file.indexOf("!/")) < 0) {
    throw new MalformedURLException();
  }
  fileURL=new URL(url.getFile().substring(0,sepIdx));
  sepIdx+=2;
  if (file.length() == sepIdx) {
    return;
  }
  entryName=file.substring(sepIdx,file.length());
  if (url.getRef() != null) {
    entryName+="#" + url.getRef();
  }
}
