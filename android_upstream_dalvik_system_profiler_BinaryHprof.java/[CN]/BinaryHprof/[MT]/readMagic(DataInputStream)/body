{
  try {
    byte[] bytes=new byte[512];
    for (int i=0; i < bytes.length; i++) {
      byte b=in.readByte();
      if (b == '\0') {
        String string=new String(bytes,0,i,"UTF-8");
        if (string.startsWith(MAGIC)) {
          return string;
        }
        return null;
      }
      bytes[i]=b;
    }
    return null;
  }
 catch (  IOException e) {
    return null;
  }
}
