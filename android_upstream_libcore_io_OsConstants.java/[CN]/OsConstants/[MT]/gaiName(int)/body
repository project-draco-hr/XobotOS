{
  if (error == EAI_AGAIN) {
    return "EAI_AGAIN";
  }
  if (error == EAI_BADFLAGS) {
    return "EAI_BADFLAGS";
  }
  if (error == EAI_FAIL) {
    return "EAI_FAIL";
  }
  if (error == EAI_FAMILY) {
    return "EAI_FAMILY";
  }
  if (error == EAI_MEMORY) {
    return "EAI_MEMORY";
  }
  if (error == EAI_NODATA) {
    return "EAI_NODATA";
  }
  if (error == EAI_NONAME) {
    return "EAI_NONAME";
  }
  if (error == EAI_OVERFLOW) {
    return "EAI_OVERFLOW";
  }
  if (error == EAI_SERVICE) {
    return "EAI_SERVICE";
  }
  if (error == EAI_SOCKTYPE) {
    return "EAI_SOCKTYPE";
  }
  if (error == EAI_SYSTEM) {
    return "EAI_SYSTEM";
  }
  return null;
}
