{
  this(Integer.MAX_VALUE);
  final ReentrantLock lock=this.lock;
  lock.lock();
  try {
    for (    E e : c) {
      if (e == null)       throw new NullPointerException();
      if (!linkLast(new Node<E>(e)))       throw new IllegalStateException("Deque full");
    }
  }
  finally {
    lock.unlock();
  }
}
