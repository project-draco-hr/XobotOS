{
  int value, next;
  if (getMaximum(field) == (next=getLeastMaximum(field))) {
    return next;
  }
  complete();
  long orgTime=time;
  set(field,next);
  do {
    value=next;
    roll(field,true);
    next=get(field);
  }
 while (next > value);
  time=orgTime;
  areFieldsSet=false;
  return value;
}
