{
  boolean addrFlag=address.getAddressType() == AddressImpl.NAME_ADDR;
  if (!addrFlag) {
    buffer.append('<');
    address.encode(buffer);
    buffer.append('>');
  }
 else {
    address.encode(buffer);
  }
  if (!parameters.isEmpty()) {
    buffer.append(SEMICOLON);
    parameters.encode(buffer);
  }
  return buffer;
}
