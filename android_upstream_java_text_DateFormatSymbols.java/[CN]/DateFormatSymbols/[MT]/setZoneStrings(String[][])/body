{
  if (zoneStrings == null) {
    throw new NullPointerException();
  }
  for (  String[] row : zoneStrings) {
    if (row.length < 5) {
      throw new IllegalArgumentException(Arrays.toString(row) + ".length < 5");
    }
  }
  this.zoneStrings=clone2dStringArray(zoneStrings);
  this.customZoneStrings=true;
}
