{
  if (provider == null || type == null || algorithm == null || className == null) {
    throw new NullPointerException();
  }
  this.provider=provider;
  this.type=type;
  this.algorithm=algorithm;
  this.className=className;
  this.aliases=((aliases != null) && (aliases.size() == 0)) ? Collections.<String>emptyList() : aliases;
  this.attributes=((attributes != null) && (attributes.size() == 0)) ? Collections.<String,String>emptyMap() : attributes;
}
