{
  if (text.equals("EST")) {
    return -5;
  }
  if (text.equals("EDT")) {
    return -4;
  }
  if (text.equals("CST")) {
    return -6;
  }
  if (text.equals("CDT")) {
    return -5;
  }
  if (text.equals("MST")) {
    return -7;
  }
  if (text.equals("MDT")) {
    return -6;
  }
  if (text.equals("PST")) {
    return -8;
  }
  if (text.equals("PDT")) {
    return -7;
  }
  return 0;
}
