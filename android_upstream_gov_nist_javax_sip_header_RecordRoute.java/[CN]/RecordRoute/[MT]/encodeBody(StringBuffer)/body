{
  if (address.getAddressType() == AddressImpl.ADDRESS_SPEC) {
    buffer.append(LESS_THAN);
  }
  address.encode(buffer);
  if (address.getAddressType() == AddressImpl.ADDRESS_SPEC) {
    buffer.append(GREATER_THAN);
  }
  if (!parameters.isEmpty()) {
    buffer.append(SEMICOLON);
    this.parameters.encode(buffer);
  }
  return buffer;
}
