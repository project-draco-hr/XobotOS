{
  if (_locationFilters == null)   _locationFilters=new ArrayList<LocationFilter>();
  if (_extraCSharpSources == null)   _extraCSharpSources=new ArrayList<String>();
  if (_sourceFolder == null)   throw new RuntimeException("<source-info> is missing <source-folder> element!");
  if (_outputFolder == null)   throw new RuntimeException("<source-info> is missing <output-folder> element!");
  return this;
}
