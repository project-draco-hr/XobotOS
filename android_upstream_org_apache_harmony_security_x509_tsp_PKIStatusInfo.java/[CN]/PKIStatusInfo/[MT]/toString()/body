{
  StringBuilder res=new StringBuilder();
  res.append("-- PKIStatusInfo:");
  res.append("\nPKIStatus : ");
  res.append(status);
  res.append("\nstatusString:  ");
  res.append(statusString);
  res.append("\nfailInfo:  ");
  res.append(failInfo);
  res.append("\n-- PKIStatusInfo End\n");
  return res.toString();
}
