{
  int hc=this.targetHost.hashCode();
  if (this.localAddress != null)   hc^=localAddress.hashCode();
  if (this.proxyChain != null) {
    hc^=proxyChain.length;
    for (int i=0; i < proxyChain.length; i++)     hc^=proxyChain[i].hashCode();
  }
  if (this.connected)   hc^=0x11111111;
  if (this.secure)   hc^=0x22222222;
  hc^=this.tunnelled.hashCode();
  hc^=this.layered.hashCode();
  return hc;
}
