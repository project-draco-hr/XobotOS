{
  byte[] data=object.getDERObject().getDEREncoded();
  this.isConstructed=explicit;
  this.tag=tag;
  if (explicit) {
    this.octets=data;
  }
 else {
    int lenBytes=getLengthOfLength(data);
    byte[] tmp=new byte[data.length - lenBytes];
    System.arraycopy(data,lenBytes,tmp,0,tmp.length);
    this.octets=tmp;
  }
}
