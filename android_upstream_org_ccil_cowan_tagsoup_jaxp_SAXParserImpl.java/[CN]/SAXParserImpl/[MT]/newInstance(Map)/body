{
  SAXParserImpl parser=new SAXParserImpl();
  if (features != null) {
    Iterator it=features.entrySet().iterator();
    while (it.hasNext()) {
      Map.Entry entry=(Map.Entry)it.next();
      parser.setFeature((String)entry.getKey(),((Boolean)entry.getValue()).booleanValue());
    }
  }
  return parser;
}
