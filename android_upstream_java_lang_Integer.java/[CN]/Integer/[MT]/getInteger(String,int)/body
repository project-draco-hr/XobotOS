{
  if (string == null || string.length() == 0) {
    return valueOf(defaultValue);
  }
  String prop=System.getProperty(string);
  if (prop == null) {
    return valueOf(defaultValue);
  }
  try {
    return decode(prop);
  }
 catch (  NumberFormatException ex) {
    return valueOf(defaultValue);
  }
}
