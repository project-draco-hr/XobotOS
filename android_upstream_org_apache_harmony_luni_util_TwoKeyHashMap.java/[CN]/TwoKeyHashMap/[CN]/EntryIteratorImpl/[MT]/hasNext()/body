{
  if (found) {
    return true;
  }
  if (curr_entry != null) {
    curr_entry=curr_entry.next;
  }
  if (curr_entry == null) {
    for (curr++; curr < arr.length && arr[curr] == null; curr++) {
    }
    if (curr < arr.length) {
      curr_entry=arr[curr];
    }
  }
  return found=(curr_entry != null);
}
