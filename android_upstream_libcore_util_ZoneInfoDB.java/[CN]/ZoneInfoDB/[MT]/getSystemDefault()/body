{
synchronized (LOCK) {
    TimezoneGetter tzGetter=TimezoneGetter.getInstance();
    String zoneName=tzGetter != null ? tzGetter.getId() : null;
    if (zoneName != null) {
      zoneName=zoneName.trim();
    }
    if (zoneName == null || zoneName.isEmpty()) {
      zoneName="localtime";
    }
    return TimeZone.getTimeZone(zoneName);
  }
}
