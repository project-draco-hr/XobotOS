{
  if (object != null) {
    for (int i=location; i < elementCount; i++) {
      if (object.equals(elementData[i])) {
        return i;
      }
    }
  }
 else {
    for (int i=location; i < elementCount; i++) {
      if (elementData[i] == null) {
        return i;
      }
    }
  }
  return -1;
}
