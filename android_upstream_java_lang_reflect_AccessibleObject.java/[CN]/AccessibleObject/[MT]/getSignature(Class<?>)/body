{
  String result="";
  String nextType=clazz.getName();
  if (trans.containsKey(nextType)) {
    result=trans.get(nextType);
  }
 else {
    if (clazz.isArray()) {
      result="[" + getSignature(clazz.getComponentType());
    }
 else {
      result="L" + nextType + ";";
    }
  }
  return result;
}
