{
  CharArrayBuffer buffer=new CharArrayBuffer(16);
  buffer.append('[');
  buffer.append(Integer.toString(this.lowerBound));
  buffer.append('>');
  buffer.append(Integer.toString(this.pos));
  buffer.append('>');
  buffer.append(Integer.toString(this.upperBound));
  buffer.append(']');
  return buffer.toString();
}
