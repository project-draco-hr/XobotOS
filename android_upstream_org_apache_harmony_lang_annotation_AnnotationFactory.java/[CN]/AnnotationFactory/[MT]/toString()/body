{
  StringBuilder result=new StringBuilder();
  result.append('@');
  result.append(klazz.getName());
  result.append('(');
  for (int i=0; i < elements.length; ++i) {
    if (i != 0) {
      result.append(", ");
    }
    result.append(elements[i]);
  }
  result.append(')');
  return result.toString();
}
