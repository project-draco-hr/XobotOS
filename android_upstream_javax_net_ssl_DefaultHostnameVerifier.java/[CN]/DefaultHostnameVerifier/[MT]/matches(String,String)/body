{
  if (cn == null) {
    return false;
  }
  cn=cn.toLowerCase(Locale.ENGLISH);
  if (cn.startsWith("*.")) {
    int matchLength=cn.length() - 1;
    return hostName.regionMatches(hostName.length() - matchLength,cn,1,matchLength) && cn.indexOf('.',2) != -1 && acceptableCountryWildcard(cn) && !InetAddress.isNumeric(hostName);
  }
 else {
    return hostName.equals(cn);
  }
}
