{
  Comparable<? super T> key=(Comparable<? super T>)x;
  int half=n >>> 1;
  while (k < half) {
    int child=(k << 1) + 1;
    Object c=array[child];
    int right=child + 1;
    if (right < n && ((Comparable<? super T>)c).compareTo((T)array[right]) > 0)     c=array[child=right];
    if (key.compareTo((T)c) <= 0)     break;
    array[k]=c;
    k=child;
  }
  array[k]=key;
}
