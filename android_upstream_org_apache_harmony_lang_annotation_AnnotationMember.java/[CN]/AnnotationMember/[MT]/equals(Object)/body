{
  if (obj == this) {
    return true;
  }
  if (obj instanceof AnnotationMember) {
    AnnotationMember that=(AnnotationMember)obj;
    if (name.equals(that.name) && tag == that.tag) {
      if (tag == ARRAY) {
        return equalArrayValue(that.value);
      }
 else       if (tag == ERROR) {
        return false;
      }
 else {
        return value.equals(that.value);
      }
    }
  }
  return false;
}
