{
  if (tag == ERROR) {
    rethrowError();
  }
  if (value == NO_VALUE) {
    return null;
  }
  if (elementType == value.getClass() || elementType.isInstance(value)) {
    return copyValue();
  }
 else {
    throw new AnnotationTypeMismatchException(definingMethod,value.getClass().getName());
  }
}
