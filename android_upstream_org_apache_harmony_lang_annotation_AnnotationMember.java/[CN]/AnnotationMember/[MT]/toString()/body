{
  if (tag == ARRAY) {
    StringBuilder sb=new StringBuilder(80);
    sb.append(name).append("=[");
    int len=Array.getLength(value);
    for (int i=0; i < len; i++) {
      if (i != 0)       sb.append(", ");
      sb.append(Array.get(value,i));
    }
    return sb.append("]").toString();
  }
 else {
    return name + "=" + value;
  }
}
