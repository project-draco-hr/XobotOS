{
synchronized (FinalizerReference.class) {
    FinalizerReference next=reference.next;
    FinalizerReference prev=reference.prev;
    reference.next=null;
    reference.prev=null;
    if (prev != null) {
      prev.next=next;
    }
 else {
      head=next;
    }
    if (next != null) {
      next.prev=prev;
    }
  }
}
