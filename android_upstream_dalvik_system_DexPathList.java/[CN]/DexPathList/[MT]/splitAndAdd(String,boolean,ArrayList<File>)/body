{
  if (path == null) {
    return;
  }
  String[] strings=path.split(Pattern.quote(File.pathSeparator));
  for (  String s : strings) {
    File file=new File(s);
    if (!(file.exists() && file.canRead())) {
      continue;
    }
    if (wantDirectories) {
      if (!file.isDirectory()) {
        continue;
      }
    }
 else {
      if (!file.isFile()) {
        continue;
      }
    }
    resultList.add(file);
  }
}
