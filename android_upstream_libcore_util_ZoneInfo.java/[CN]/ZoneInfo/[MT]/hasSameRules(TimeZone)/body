{
  if (!(timeZone instanceof ZoneInfo)) {
    return false;
  }
  ZoneInfo other=(ZoneInfo)timeZone;
  if (mUseDst != other.mUseDst) {
    return false;
  }
  if (!mUseDst) {
    return mRawOffset == other.mRawOffset;
  }
  return mRawOffset == other.mRawOffset && Arrays.equals(mOffsets,other.mOffsets) && Arrays.equals(mIsDsts,other.mIsDsts) && Arrays.equals(mTypes,other.mTypes) && Arrays.equals(mTransitions,other.mTransitions);
}
