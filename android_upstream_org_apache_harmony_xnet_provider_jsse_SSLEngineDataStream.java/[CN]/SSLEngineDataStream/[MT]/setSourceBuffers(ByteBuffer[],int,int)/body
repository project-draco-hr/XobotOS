{
  this.srcs=srcs;
  this.offset=offset;
  this.limit=offset + length;
  this.consumed=0;
  this.available=0;
  for (int i=offset; i < limit; i++) {
    if (srcs[i] == null) {
      throw new IllegalStateException("Some of the input parameters are null");
    }
    available+=srcs[i].remaining();
  }
}
