{
  ArrayList<URL> list=new ArrayList<URL>();
  int numEntries=getBootClassPathSize();
  for (int i=0; i < numEntries; i++) {
    String urlStr=getBootClassPathResource(name,i);
    if (urlStr != null) {
      try {
        list.add(new URL(urlStr));
      }
 catch (      MalformedURLException mue) {
        mue.printStackTrace();
      }
    }
  }
  return list;
}
