{
synchronized (this) {
    if (called) {
      return;
    }
    called=true;
  }
  System.err.println(this.getClass().getName() + ": " + FAILURES[errorCode]);
  if (message != null) {
    System.err.println("Error message - " + message);
  }
  if (exception != null) {
    System.err.println("Exception - " + exception);
  }
}
