{
  String temp="";
  for (int i=0; i < ip.length / 2; i++) {
    temp+=Integer.toString(ip[i] & 0x00FF) + ".";
  }
  temp=temp.substring(0,temp.length() - 1);
  temp+="/";
  for (int i=ip.length / 2; i < ip.length; i++) {
    temp+=Integer.toString(ip[i] & 0x00FF) + ".";
  }
  temp=temp.substring(0,temp.length() - 1);
  return temp;
}
