{
  if (start >= 0 && end <= size()) {
    if (start <= end) {
      if (this instanceof RandomAccess) {
        return new SubAbstractListRandomAccess<E>(this,start,end);
      }
      return new SubAbstractList<E>(this,start,end);
    }
    throw new IllegalArgumentException();
  }
  throw new IndexOutOfBoundsException();
}
