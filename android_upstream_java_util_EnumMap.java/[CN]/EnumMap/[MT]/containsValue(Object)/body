{
  if (value == null) {
    for (int i=0; i < enumSize; i++) {
      if (hasMapping[i] && values[i] == null) {
        return true;
      }
    }
  }
 else {
    for (int i=0; i < enumSize; i++) {
      if (hasMapping[i] && value.equals(values[i])) {
        return true;
      }
    }
  }
  return false;
}
