{
  if (array == null) {
    return "null";
  }
  if (array.length == 0) {
    return "[]";
  }
  StringBuilder sb=new StringBuilder(array.length * 7);
  sb.append('[');
  sb.append(array[0]);
  for (int i=1; i < array.length; i++) {
    sb.append(", ");
    sb.append(array[i]);
  }
  sb.append(']');
  return sb.toString();
}
