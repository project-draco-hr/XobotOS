{
  StringBuilder buf=new StringBuilder(50);
  buf.append("Version: ");
  buf.append(Integer.toHexString(getVersionNumber()));
  buf.append(" Command: ");
  buf.append(Integer.toHexString(getCommandOrResult()));
  buf.append(" Port: ");
  buf.append(getPort());
  buf.append(" IP: ");
  buf.append(Integer.toHexString(getIP()));
  buf.append(" User ID: ");
  buf.append(getUserId());
  return buf.toString();
}
