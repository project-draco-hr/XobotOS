{
  if (o == null)   return false;
  final Object[] items=this.items;
  final ReentrantLock lock=this.lock;
  lock.lock();
  try {
    for (int i=takeIndex, k=count; k > 0; i=inc(i), k--) {
      if (o.equals(items[i])) {
        removeAt(i);
        return true;
      }
    }
    return false;
  }
  finally {
    lock.unlock();
  }
}
