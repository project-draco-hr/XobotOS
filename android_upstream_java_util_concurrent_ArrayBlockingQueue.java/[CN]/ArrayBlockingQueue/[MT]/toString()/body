{
  final ReentrantLock lock=this.lock;
  lock.lock();
  try {
    int k=count;
    if (k == 0)     return "[]";
    StringBuilder sb=new StringBuilder();
    sb.append('[');
    for (int i=takeIndex; ; i=inc(i)) {
      Object e=items[i];
      sb.append(e == this ? "(this Collection)" : e);
      if (--k == 0)       return sb.append(']').toString();
      sb.append(',').append(' ');
    }
  }
  finally {
    lock.unlock();
  }
}
