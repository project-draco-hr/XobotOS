{
  if (a1 == null) {
    if (a2 == null) {
      return true;
    }
 else {
      return false;
    }
  }
 else {
    if (a2 != null && a1.length == a2.length) {
      for (int i=0; i < a1.length; i++) {
        if (!equals(a1[i],a2[i])) {
          return false;
        }
      }
      return true;
    }
 else {
      return false;
    }
  }
}
