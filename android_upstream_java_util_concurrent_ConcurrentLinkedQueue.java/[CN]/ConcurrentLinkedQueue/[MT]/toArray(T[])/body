{
  int k=0;
  Node<E> p;
  for (p=first(); p != null && k < a.length; p=succ(p)) {
    E item=p.item;
    if (item != null)     a[k++]=(T)item;
  }
  if (p == null) {
    if (k < a.length)     a[k]=null;
    return a;
  }
  ArrayList<E> al=new ArrayList<E>();
  for (Node<E> q=first(); q != null; q=succ(q)) {
    E item=q.item;
    if (item != null)     al.add(item);
  }
  return al.toArray(a);
}
