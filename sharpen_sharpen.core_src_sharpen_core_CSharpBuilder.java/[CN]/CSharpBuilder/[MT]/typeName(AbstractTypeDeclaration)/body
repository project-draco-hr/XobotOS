{
  String renamed=annotatedRenaming(node);
  if (renamed != null)   return renamed;
  renamed=mappedTypeName(node.resolveBinding());
  if (renamed != null) {
    int i=renamed.lastIndexOf('.');
    if (i != -1)     return renamed.substring(i + 1);
 else     return renamed;
  }
  return node.getName().toString();
}
