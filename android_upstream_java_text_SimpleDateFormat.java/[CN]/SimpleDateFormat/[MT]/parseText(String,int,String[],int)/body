{
  int found=-1;
  for (int i=0; i < text.length; i++) {
    if (text[i].isEmpty()) {
      continue;
    }
    if (string.regionMatches(true,offset,text[i],0,text[i].length())) {
      if (found == -1 || text[i].length() > text[found].length()) {
        found=i;
      }
    }
  }
  if (found != -1) {
    calendar.set(field,found);
    return offset + text[found].length();
  }
  return -offset - 1;
}
