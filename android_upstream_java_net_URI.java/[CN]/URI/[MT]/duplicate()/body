{
  URI clone=new URI();
  clone.absolute=absolute;
  clone.authority=authority;
  clone.fragment=fragment;
  clone.host=host;
  clone.opaque=opaque;
  clone.path=path;
  clone.port=port;
  clone.query=query;
  clone.scheme=scheme;
  clone.schemeSpecificPart=schemeSpecificPart;
  clone.userInfo=userInfo;
  clone.serverAuthority=serverAuthority;
  return clone;
}
