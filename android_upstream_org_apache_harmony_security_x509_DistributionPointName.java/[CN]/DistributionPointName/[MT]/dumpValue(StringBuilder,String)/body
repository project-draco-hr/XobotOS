{
  sb.append(prefix);
  sb.append("Distribution Point Name: [\n");
  if (fullName != null) {
    fullName.dumpValue(sb,prefix + "  ");
  }
 else {
    sb.append(prefix);
    sb.append("  ");
    sb.append(nameRelativeToCRLIssuer.getName(X500Principal.RFC2253));
  }
  sb.append(prefix);
  sb.append("]\n");
}
