{
  int length=s.length();
  StringBuilder result=new StringBuilder(length);
  for (int i=0; i < length; ++i) {
    char ch=s.charAt(i);
    int value=Character.digit(ch,base);
    if (value >= 0 && value <= 9) {
      ch=(char)('0' + value);
    }
    result.append(ch);
  }
  return result.toString();
}
