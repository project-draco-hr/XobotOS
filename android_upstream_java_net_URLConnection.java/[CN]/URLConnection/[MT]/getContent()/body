{
  if (!connected) {
    connect();
  }
  if ((contentType=getContentType()) == null) {
    if ((contentType=guessContentTypeFromName(url.getFile())) == null) {
      contentType=guessContentTypeFromStream(getInputStream());
    }
  }
  if (contentType != null) {
    return getContentHandler(contentType).getContent(this);
  }
  return null;
}
