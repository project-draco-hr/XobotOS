{
  if (length == 0) {
    fatalAlert(AlertProtocol.DECODE_ERROR,"DECODE ERROR: incorrect CertificateVerify");
  }
 else {
    if (in.readUint16() != length - 2) {
      fatalAlert(AlertProtocol.DECODE_ERROR,"DECODE ERROR: incorrect CertificateVerify");
    }
    signedHash=in.read(length - 2);
  }
  this.length=length;
}
