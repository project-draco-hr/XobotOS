{
  String svalue=get(key,null);
  if (svalue == null) {
    return deflt;
  }
  if (svalue.length() == 0) {
    return EmptyArray.BYTE;
  }
  try {
    byte[] bavalue=svalue.getBytes(Charsets.US_ASCII);
    if (bavalue.length % 4 != 0) {
      return deflt;
    }
    return Base64.decode(bavalue);
  }
 catch (  Exception e) {
    return deflt;
  }
}
