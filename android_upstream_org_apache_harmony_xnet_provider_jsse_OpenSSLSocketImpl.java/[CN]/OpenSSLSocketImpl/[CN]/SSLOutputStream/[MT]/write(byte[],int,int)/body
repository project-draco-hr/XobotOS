{
  BlockGuard.getThreadPolicy().onNetwork();
synchronized (writeLock) {
    checkOpen();
    Arrays.checkOffsetAndCount(buf.length,offset,byteCount);
    if (byteCount == 0) {
      return;
    }
    NativeCrypto.SSL_write(sslNativePointer,socket.getFileDescriptor$(),OpenSSLSocketImpl.this,buf,offset,byteCount);
  }
}
