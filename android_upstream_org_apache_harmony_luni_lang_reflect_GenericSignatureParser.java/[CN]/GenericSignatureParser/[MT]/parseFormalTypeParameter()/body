{
  scanIdentifier();
  String name=identifier.intern();
  ListOfTypes bounds=new ListOfTypes(8);
  expect(':');
  if (symbol == 'L' || symbol == '[' || symbol == 'T') {
    bounds.add(parseFieldTypeSignature());
  }
  while (symbol == ':') {
    scanSymbol();
    bounds.add(parseFieldTypeSignature());
  }
  return new ImplForVariable<GenericDeclaration>(genericDecl,name,bounds);
}
