{
  Class<?> search=cl;
  Method method=null;
  while (search != null) {
    try {
      method=search.getDeclaredMethod(methodName,(Class[])null);
      if (search == cl || (method.getModifiers() & Modifier.PRIVATE) == 0) {
        method.setAccessible(true);
        return method;
      }
    }
 catch (    NoSuchMethodException nsm) {
    }
    search=search.getSuperclass();
  }
  return null;
}
